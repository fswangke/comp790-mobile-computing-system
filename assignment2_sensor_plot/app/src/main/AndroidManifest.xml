<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="unc.edu.kewang.sensorplot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sensor_plot"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sensoractivity.AccelerometerSensorDetailActivity" />
        <activity android:name=".sensoractivity.LightSensorDetailActivity" />
        <activity android:name=".sensoractivity.OrientationSensorDetailActivity" />
        <activity android:name=".sensoractivity.SensorDetailActivity" />
    </application>

</manifest>